<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraint_layout_add_med_week_days"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".AddMedWeekDaysFragment">

    <include layout="@layout/add_med_toolbar"/>

    <include
        android:id="@+id/include6"
        layout="@layout/add_med_header"
        tools:layout_editor_absoluteY="56dp" />


    <include
        layout="@layout/add_med_next_button"/>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recycler_view_week_days_add_med"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="32dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/include6" />

</androidx.constraintlayout.widget.ConstraintLayout>